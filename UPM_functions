for i in ${!found18[@]} ; do package_check_repo "${found18[i]}" > /dev/null ;  package+=("${found18[i]}") ; done

found19=($(printf '%s\n' "${treelistnew19[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found19[@]} ; do package_check_repo "${found19[i]}" > /dev/null ;  package+=("${found19[i]}") ; done

