    echo

package_check_repo() {

if [ $(printf '%s\n' "found ${treelistnew0[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL0$1
    ftp=$ftpPOOL0$1
    echo $1 found in $POOL0

elif [ $(printf '%s\n' "${treelistnew1[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL1$1
    ftp=$ftpPOOL1$1
    echo $1 found in $POOL1

elif [ $(printf '%s\n' "${treelistnew2[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL2$1
    ftp=$ftpPOOL2$1
    echo $1 found in $POOL2

elif [ $(printf '%s\n' "${treelistnew3[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL3$1
    ftp=$ftpPOOL3$1
    echo $1 found in $POOL3

elif [ $(printf '%s\n' "${treelistnew4[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL4$1
    ftp=$ftpPOOL4$1
    echo $1 found in $POOL4

elif [ $(printf '%s\n' "${treelistnew5[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL5$1
    ftp=$ftpPOOL5$1
    echo $1 found in $POOL5

elif [ $(printf '%s\n' "${treelistnew6[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL6$1
    ftp=$ftpPOOL6$1
    echo $1 found in $POOL6

elif [ $(printf '%s\n' "${treelistnew7[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL7$1
    ftp=$ftpPOOL7$1
    echo $1 found in $POOL7

elif [ $(printf '%s\n' "${treelistnew8[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL8$1
    ftp=$ftpPOOL8$1
    echo $1 found in $POOL8

elif [ $(printf '%s\n' "${treelistnew9[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL9$1
    ftp=$ftpPOOL9$1
    echo $1 found in $POOL9

elif [ $(printf '%s\n' "${treelistnew10[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL10$1
    ftp=$ftpPOOL10$1
    echo $1 found in $POOL10

elif [ $(printf '%s\n' "${treelistnew11[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL11$1
    ftp=$ftpPOOL11$1
    echo $1 found in $POOL11

elif [ $(printf '%s\n' "${treelistnew12[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL12$1
    ftp=$ftpPOOL12$1
    echo $1 found in $POOL12

elif [ $(printf '%s\n' "${treelistnew13[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL13$1
    ftp=$ftpPOOL13$1
    echo $1 found in $POOL13

elif [ $(printf '%s\n' "${treelistnew14[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL14$1
    ftp=$ftpPOOL14$1
    echo $1 found in $POOL14

elif [ $(printf '%s\n' "${treelistnew15[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL15$1
    ftp=$ftpPOOL15$1
    echo $1 found in $POOL15

elif [ $(printf '%s\n' "${treelistnew16[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL16$1
    ftp=$ftpPOOL16$1
    echo $1 found in $POOL16

elif [ $(printf '%s\n' "${treelistnew17[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL17$1
    ftp=$ftpPOOL17$1
    echo $1 found in $POOL17

elif [ $(printf '%s\n' "${treelistnew18[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL18$1
    ftp=$ftpPOOL18$1
    echo $1 found in $POOL18

elif [ $(printf '%s\n' "${treelistnew19[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL19$1
    ftp=$ftpPOOL19$1
    echo $1 found in $POOL19

elif [ $(printf '%s\n' "${treelistnew20[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL20$1
    ftp=$ftpPOOL20$1
    echo $1 found in $POOL20

elif [ $(printf '%s\n' "${treelistnew21[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL21$1
    ftp=$ftpPOOL21$1
    echo $1 found in $POOL21

elif [ $(printf '%s\n' "${treelistnew22[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL22$1
    ftp=$ftpPOOL22$1
    echo $1 found in $POOL22

elif [ $(printf '%s\n' "${treelistnew23[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL23$1
    ftp=$ftpPOOL23$1
    echo $1 found in $POOL23

elif [ $(printf '%s\n' "${treelistnew24[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL24$1
    ftp=$ftpPOOL24$1
    echo $1 found in $POOL24

elif [ $(printf '%s\n' "${treelistnew25[@]}" | grep -i -e ^"$1") ]
    then
    url=$POOL25$1
    ftp=$ftpPOOL25$1
    echo $1 found in $POOL25

else
    echo $1 not found
    notfound+=($1)
fi
}

package_search_repo() {
echo "initiating search for $1"
found0=($(printf '%s\n' "${treelistnew0[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found0[@]}
    do
        package_check_repo "${found0[i]}" > /dev/null
printf '%b\n' "Repository: blackarch, \
\
Number: 0, \
\
Section: Could Not Detect, \
\
Package Availability: $(package_check_repo ${found0[i]} | grep -q 'found in' ; echo $? | sed -e 's/0/Available,     /g' -e 's/1/Not Available, /g')\
\
Package: "${found0[i]}" \
"
done

found1=($(printf '%s\n' "${treelistnew1[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found1[@]}
    do
        package_check_repo "${found1[i]}" > /dev/null
printf '%b\n' "Repository: chakra, \
\
Number: 1, \
\
Section: core, \
\
Package Availability: $(package_check_repo ${found1[i]} | grep -q 'found in' ; echo $? | sed -e 's/0/Available,     /g' -e 's/1/Not Available, /g')\
\
Package: "${found1[i]}" \
"
done

