for i in ${!found21[@]} ; do package_check_repo "${found21[i]}" > /dev/null ;  package+=("${found21[i]}") ; done

found22=($(printf '%s\n' "${treelistnew22[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found22[@]} ; do package_check_repo "${found22[i]}" > /dev/null ;  package+=("${found22[i]}") ; done

found23=($(printf '%s\n' "${treelistnew23[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found23[@]} ; do package_check_repo "${found23[i]}" > /dev/null ;  package+=("${found23[i]}") ; done

found24=($(printf '%s\n' "${treelistnew24[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found24[@]} ; do package_check_repo "${found24[i]}" > /dev/null ;  package+=("${found24[i]}") ; done

found25=($(printf '%s\n' "${treelistnew25[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found25[@]} ; do package_check_repo "${found25[i]}" > /dev/null ;  package+=("${found25[i]}") ; done


package=($(printf '%b\n' ${package[@]} | sort -u))
if [[ -z $url || -z $ftp ]]
    then
        echo $1 not found | sed 's/-\[0-9\]//g'
        notfound+=($(echo $1 | sed 's/-\[0-9\]//g'))
        return 1
    else
        echo $1 found | sed 's/-\[0-9\]//g'
#         declare -p url ftp
        return 0
fi
}
