ftpPOOL23='ftp://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/'
echo "connecting to http://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/" >&2
    online="$(date -d "$(curl -k -I "" |& grep -i "modified" | sed 's/.*\: //')" +%s)"
    physical="$(cat ./repo/http://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/timestamp | sed 's/.*: //g')"
    difference="$(($physical-$online))"

#     curl -k -I "" |& grep -i "modified" >&2 || curl -k -I "" >&2
#     echo "Last-Modified: $online (online)" >&2
#     echo "Last-Modified: $physical (physical)" >&2
#     echo "Last-Modified: $(date -d "$(date +'%a, %d %b %Y %H:%M:%S')" +%s) (current)" >&2
#     echo "difference: $difference seconds" >&2
    if [[ $difference =~ - ]]
        then
            keyword=newer
            difference=${difference[0]/-/}
        else
            keyword=older
    fi
    echo "online repo is $keyword then cached repo by $difference seconds" >&2
    if [[ $keyword =~ "older" ]]
        then
            echo "repo does not need updating" >&2
            treelistnew23=($(./Files/lynx/lynx -cfg ./Files/lynx/lynx.cfg -lss ./Files/lynx/lynx.lss -dump -listonly -nonumbers './repo/http://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/index.html' | sed "s/.*x86_64\///g" | grep "xz$"))
        else
            echo "repo needs updating" >&2
            aria2c $max_speed_flags_html_repo http://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/ -o ./repo/http://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/index.html
            response=0
            if [[ $response == 0 ]]
                then
                    treelistnew23=($(./Files/lynx/lynx -cfg ./Files/lynx/lynx.cfg -lss ./Files/lynx/lynx.lss -dump -listonly -nonumbers 'http://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/' | sed 's/http:\/\/www-ftp.lip6.fr\/pub\/linux\/distributions\/chakra\/lib32\/x86_64\///g' | grep "xz$"))
                    mkdir -pv ./repo/http://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/
                    echo "Last-Modified: $(date -d "$(date +'%a, %d %b %Y %H:%M:%S')" +%s)" >./repo/http://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/timestamp
                    printf "\rDownloaded 'http://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/'    \n"
                else
                    echo "repo could not be downloaded" >&2
            fi
    fi
    echo
POOL24='http://mirror.dacentec.com/chakra/lib32/x86_64/'
ftpPOOL24='ftp://mirror.dacentec.com/chakra/lib32/x86_64/'
echo "connecting to http://mirror.dacentec.com/chakra/lib32/x86_64/" >&2
    online="$(date -d "$(curl -k -I "" |& grep -i "modified" | sed 's/.*\: //')" +%s)"
    physical="$(cat ./repo/http://mirror.dacentec.com/chakra/lib32/x86_64/timestamp | sed 's/.*: //g')"
