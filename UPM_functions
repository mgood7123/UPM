package_quick_search_repo() {
unset url ftp
found0=($(printf '%s\n' "${treelistnew0[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found0[@]} ; do package_check_repo "${found0[i]}" > /dev/null ;  package+=("${found0[i]}") ; done

found1=($(printf '%s\n' "${treelistnew1[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found1[@]} ; do package_check_repo "${found1[i]}" > /dev/null ;  package+=("${found1[i]}") ; done

found2=($(printf '%s\n' "${treelistnew2[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found2[@]} ; do package_check_repo "${found2[i]}" > /dev/null ;  package+=("${found2[i]}") ; done

found3=($(printf '%s\n' "${treelistnew3[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found3[@]} ; do package_check_repo "${found3[i]}" > /dev/null ;  package+=("${found3[i]}") ; done

found4=($(printf '%s\n' "${treelistnew4[@]}" | grep -i -e ^"$1" | grep -i -e "64" -i -e "any"))
for i in ${!found4[@]} ; do package_check_repo "${found4[i]}" > /dev/null ;  package+=("${found4[i]}") ; done

