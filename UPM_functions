for the following two checks (we check for the presence of readlink, and dirname), we assume that the current working directory is set to the location of this script should any of them fail...
dirname exists
readlink exists
script directory = /home/chakra/UPM
REPOS=(
https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/
# http://mirrors.kernel.org/archlinux/pool/packages/
http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/
http://mirror.dacentec.com/chakra/core/x86_64/
http://ftp.acc.umu.se/mirror/chakraos.org/packages/core/x86_64/

#https://mirror.aarnet.edu.au/pub/archlinux/core/os/x86_64/
http://archlinux.mirror.digitalpacific.com.au/core/os/x86_64/
http://mirror.internode.on.net/pub/archlinux/core/os/x86_64/
http://archlinux.melbourneitmirror.net/core/os/x86_64/
http://ftp.swin.edu.au/archlinux/core/os/x86_64/

#https://mirror.aarnet.edu.au/pub/archlinux/community/os/x86_64/
http://archlinux.mirror.digitalpacific.com.au/community/os/x86_64/
http://mirror.internode.on.net/pub/archlinux/community/os/x86_64/
http://archlinux.melbourneitmirror.net/community/os/x86_64/
http://ftp.swin.edu.au/archlinux/community/os/x86_64/

#https://mirror.aarnet.edu.au/pub/archlinux/extra/os/x86_64/
http://archlinux.mirror.digitalpacific.com.au/extra/os/x86_64/
http://mirror.internode.on.net/pub/archlinux/extra/os/x86_64/
http://archlinux.melbourneitmirror.net/extra/os/x86_64/
http://ftp.swin.edu.au/archlinux/extra/os/x86_64/

#https://mirror.aarnet.edu.au/pub/archlinux/multilib/os/x86_64/
http://archlinux.mirror.digitalpacific.com.au/multilib/os/x86_64/
http://mirror.internode.on.net/pub/archlinux/multilib/os/x86_64/
http://archlinux.melbourneitmirror.net/multilib/os/x86_64/
http://ftp.swin.edu.au/archlinux/multilib/os/x86_64/

http://www-ftp.lip6.fr/pub/linux/distributions/chakra/desktop/x86_64/
http://mirror.dacentec.com/chakra/desktop/x86_64/
http://ftp.acc.umu.se/mirror/chakraos.org/packages/desktop/x86_64/

http://www-ftp.lip6.fr/pub/linux/distributions/chakra/lib32/x86_64/
http://mirror.dacentec.com/chakra/lib32/x86_64/
http://ftp.acc.umu.se/mirror/chakraos.org/packages/lib32/x86_64/
)
REPODB=(
https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/blackarch.db
$(for i in ${!REPOS[@]}
do
    if [[ ${REPOS[i]} =~ "/core/" ]]
        then
            printf "${REPOS[i]}${REPOS[i]/*\/core\/*/core}.db
"
    elif [[ ${REPOS[i]} =~ "/community/" ]]
        then
            printf "${REPOS[i]}${REPOS[i]/*\/community\/*/community}.db
"
    elif [[ ${REPOS[i]} =~ "/extra/" ]]
        then
            printf "${REPOS[i]}${REPOS[i]/*\/extra\/*/extra}.db
"
    elif [[ ${REPOS[i]} =~ "/multilib/" ]]
        then
            printf "${REPOS[i]}${REPOS[i]/*\/multilib\/*/multilib}.db
"
    fi
done)
)
POOL0='https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/'
ftpPOOL0='ftp://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/'
echo "connecting to https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/" >&2
    online="$(date -d "$(curl -k -I "https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/blackarch.db" |& grep -i "modified" | sed 's/.*\: //')" +%s)"
    physical="$(cat ./repo/https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/timestamp | sed 's/.*: //g')"
    difference="$(($physical-$online))"

#     curl -k -I "https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/blackarch.db" |& grep -i "modified" >&2 || curl -k -I "https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/blackarch.db" >&2
#     echo "Last-Modified: $online (online)" >&2
#     echo "Last-Modified: $physical (physical)" >&2
#     echo "Last-Modified: $(date -d "$(date +'%a, %d %b %Y %H:%M:%S')" +%s) (current)" >&2
#     echo "difference: $difference seconds" >&2
    if [[ $difference =~ - ]]
        then
            keyword=newer
            difference=${difference[0]/-/}
        else
            keyword=older
    fi
    echo "online repo is $keyword then cached repo by $difference seconds" >&2
    if [[ $keyword =~ "older" ]]
        then
            echo "repo does not need updating" >&2
            treelistnew0=($(./Files/lynx/lynx -cfg ./Files/lynx/lynx.cfg -lss ./Files/lynx/lynx.lss -dump -listonly -nonumbers './repo/https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/index.html' | sed "s/.*x86_64\///g" | grep "xz$"))
        else
            echo "repo needs updating" >&2
            aria2c $max_speed_flags_html_repo https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/ -o ./repo/https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/index.html
            response=0
            if [[ $response == 0 ]]
                then
                    treelistnew0=($(./Files/lynx/lynx -cfg ./Files/lynx/lynx.cfg -lss ./Files/lynx/lynx.lss -dump -listonly -nonumbers 'https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/' | sed 's/https:\/\/www.mirrorservice.org\/sites\/blackarch.org\/blackarch\/blackarch\/os\/x86_64\///g' | grep "xz$"))
                    mkdir -pv ./repo/https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/
                    echo "Last-Modified: $(date -d "$(date +'%a, %d %b %Y %H:%M:%S')" +%s)" >./repo/https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/timestamp
                    printf "\rDownloaded 'https://www.mirrorservice.org/sites/blackarch.org/blackarch/blackarch/os/x86_64/'    \n"
                else
                    echo "repo could not be downloaded" >&2
            fi
    fi
    echo
POOL1='http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/'
ftpPOOL1='ftp://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/'
echo "connecting to http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/" >&2
    online="$(date -d "$(curl -k -I "http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/core.db" |& grep -i "modified" | sed 's/.*\: //')" +%s)"
    physical="$(cat ./repo/http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/timestamp | sed 's/.*: //g')"
    difference="$(($physical-$online))"

#     curl -k -I "http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/core.db" |& grep -i "modified" >&2 || curl -k -I "http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/core.db" >&2
#     echo "Last-Modified: $online (online)" >&2
#     echo "Last-Modified: $physical (physical)" >&2
#     echo "Last-Modified: $(date -d "$(date +'%a, %d %b %Y %H:%M:%S')" +%s) (current)" >&2
#     echo "difference: $difference seconds" >&2
    if [[ $difference =~ - ]]
        then
            keyword=newer
            difference=${difference[0]/-/}
        else
            keyword=older
    fi
    echo "online repo is $keyword then cached repo by $difference seconds" >&2
    if [[ $keyword =~ "older" ]]
        then
            echo "repo does not need updating" >&2
            treelistnew1=($(./Files/lynx/lynx -cfg ./Files/lynx/lynx.cfg -lss ./Files/lynx/lynx.lss -dump -listonly -nonumbers './repo/http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/index.html' | sed "s/.*x86_64\///g" | grep "xz$"))
        else
            echo "repo needs updating" >&2
            aria2c $max_speed_flags_html_repo http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/ -o ./repo/http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/index.html
            response=0
            if [[ $response == 0 ]]
                then
                    treelistnew1=($(./Files/lynx/lynx -cfg ./Files/lynx/lynx.cfg -lss ./Files/lynx/lynx.lss -dump -listonly -nonumbers 'http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/' | sed 's/http:\/\/www-ftp.lip6.fr\/pub\/linux\/distributions\/chakra\/core\/x86_64\///g' | grep "xz$"))
                    mkdir -pv ./repo/http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/
                    echo "Last-Modified: $(date -d "$(date +'%a, %d %b %Y %H:%M:%S')" +%s)" >./repo/http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/timestamp
                    printf "\rDownloaded 'http://www-ftp.lip6.fr/pub/linux/distributions/chakra/core/x86_64/'    \n"
                else
                    echo "repo could not be downloaded" >&2
            fi
    fi
    echo
